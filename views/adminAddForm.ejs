<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/style.css">
  </head>
  <body>
    <div id="hr-main-div">
      <form method="post" action="/form/submit/admin">
        <div id="form-info-box">
          <div>
            <label for="userID">UserID*</label>
            <input
              type="text"
              name="userID"
              id="userID"
              placeholder="User ID"
              required
            />
          </div>

          <div>
            <label for="name">Name</label>
            <input
              type="text"
              name="name"
              id="name"
              placeholder="Name"
            />
          </div>


          <div>
            <button id="form-button" class="btn">Add Now</button>
          </div>
        </div>
      </form>
    </div>
  </body>
</html>
